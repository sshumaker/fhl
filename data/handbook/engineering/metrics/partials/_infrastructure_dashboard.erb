<% if ENV['PERISCOPE_EMBED_API_KEY'] %>
  <div>
    <embed width="100%" height="800" src="<%= signed_periscope_url({ dashboard: 798401, embed: 'v2', filters: [{name: filter_type, value: filter_value }], visible: ["development_section", "team_group", "stage"], data_ts: Date.today.to_time.to_i }) %>">
  </div>
  <% else %>
    <p>You must set a <code>PERISCOPE_EMBED_API_KEY</code> environment variable to render this chart.</p>
<% end %>
